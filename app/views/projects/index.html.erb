<h1>TO Do List</h1>



<% @projects.each do |project| %>
  <h2><%= link_to project.name , project %></h2>
    <ul>
    <% for task in project.tasks %>
       <li>task.name</li>
     <% end %>   
   </ul>
  <%= link_to 'Edit', edit_project_path(project) %>
<% end %>
